            //-webkit-overflow-scrolling touch
        intro
            css
                width 100%
                background-color #161616
            logo
                css
                    transform scale(1.025)
                    animation logopulse 3.5s ease-in-out infinite
                    margin 0 auto
                    cursor crosshair
            title
                css
                    font-size 150px
                    color #E5E5E5
                    //line-height 85px
            text
                css
                    font-size 24px
                    color #E6E6E6
                textL
                    css
                        font-size inherit
                space
                    css
                        font-size inherit
                textR
                    css
                        font-size inherit
            spacer
                css
                    height 13%
            nav
                css
                    height 70px
                    position absolute
                    bottom 12px
                    left 0
                    right 0
                    cursor pointer
                    transition opacity 0.3s ease-in-out
                    -moz-transition opacity 0.3s ease-in-out
                    -webkit-transition opacity 0.3s ease-in-out
                    -webkit-backface-visibility hidden
                    animation none
                text
                    css
                        font 25px JosefinSans
                        color #DFDFDF
                        display block
                down
                    css
                        margin 10px auto
                        opacity 0.65
                        transition opacity 0.3s ease-in-out
                        -moz-transition opacity 0.3s ease-in-out
                        -webkit-transition opacity 0.3s ease-in-out
                        height 35px
                        width 60px
